{
  "name": "k6-performance-tests",
  "version": "1.0.0",
  "description": "K6 Performance Testing Suite for API Mobile",
  "scripts": {
    "test:smoke": "k6 run scenarios/smoke.test.js",
    "test:load": "k6 run scenarios/load.test.js",
    "test:stress": "k6 run scenarios/stress.test.js",
    "test:spike": "k6 run scenarios/spike.test.js",
    "test:soak": "k6 run scenarios/soak.test.js",
    "test:all": "npm run test:smoke && npm run test:load && npm run test:stress",
    "test:report": "k6 run --out html=results/report.html scenarios/load.test.js",
    "test:json": "k6 run --out json=results/results.json scenarios/load.test.js"
  },
  "keywords": [
    "k6",
    "performance",
    "load-testing",
    "stress-testing",
    "api-testing"
